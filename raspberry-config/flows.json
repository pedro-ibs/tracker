    {
        "id": "57819dfe4ae9e916",
        "type": "function",
        "z": "1b74ef80f309d060",
        "name": "motor left",
        "func": "msg.motor1_left         = 0;\nmsg.motor2_left         = 0;\nmsg.motor_left_conjunct = 0;\n\nif (msg.payload.value > 1){\n    msg.motor1_left         = msg.payload.value * (2);\n    msg.motor_left_conjunct = msg.motor1_left;\n}\n\nif (msg.payload.value < -1) {\n    msg.motor2_left         = msg.payload.value * (-2);\n    msg.motor_left_conjunct = msg.motor2_left;\n\n}\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 1060,
        "y": 80,
        "wires": [
